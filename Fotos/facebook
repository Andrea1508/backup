	            <div class="tooltip__content tooltip__content--permanent">
	                <div class="content">
	                    <span class="modal-trigger" data-modal="modal-premium" onclick="ga('send', 'event', 'Plans', 'What is this?')">¿Qué es esto?</span>
	                </div>
	            </div>
	        </span>
	        {{/specialItem}}
	    </div>
	</li>
</script>
